<header>
  <h1>View or update your account</h1>
</header>


<form>
  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="inputFirst">First Name</label>
      <input type="text" class="form-control" id="inputFirst" placeholder= "{{first_name}}">
    </div>
    <div class="form-group col-md-6">
      <label for="inputLast">Last Name</label>
      <input type="text" class="form-control" id="inputLast" placeholder= "{{last_name}}""LastName">
    </div>
  </div>

  <div class="form-row">
    <div class="form-group col-md-6">
      <label for="inputEmail">Email</label>
      <input type="email" class="form-control" id="inputEmail" placeholder= "{{email}}""Email">
    </div>
    <div class="form-group col-md-6">
      <label for="inputPhone">Phone</label>

      <input type="text" class="form-control" id="inputPhone" placeholder= "{{phone}}">
    </div>
  </div>

